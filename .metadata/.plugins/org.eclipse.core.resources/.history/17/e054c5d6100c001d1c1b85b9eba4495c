package Practise;

public class SortingElements {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s= "Clean world green world";
		String res="";
		
		for(int i=s.length() -1;i>=0;i++)
		{
			char ch = s.charAt(i);
			res=res+ch;
						
		}
		System.out.println(res);
		
		

	}

}
