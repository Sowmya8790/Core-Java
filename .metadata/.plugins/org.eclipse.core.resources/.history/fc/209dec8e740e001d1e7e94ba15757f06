package Week1;

public class ASCII {

	public static void main(String[] args) {
		
		int ASCI1 = 'A';
		int ASCI2 = 'b';
	
		
		
		for(int i=0;i<=99;i++)
		{
			System.out.println((char)i +" "+i);
		}
		
		//Type casting
		
		char c1='a';
		char c2='b';
		
		int asci= (int) c1;
		
		System.out.println(asci);
		
		
	

	}

}
